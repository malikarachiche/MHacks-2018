target 'MHacks StockX' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
    pod 'Alamofire', '~> 4.7'
    pod 'SwiftyJSON', '~> 4.0'
	
    pod ‘Firebase/Core’
    pod ‘Firebase/Database’
    pod ‘Firebase/Auth’
    pod ‘Firebase/Storage’
    pod 'SwiftyUUID'

  # Pods for MHacks StockX

  target 'MHacks StockXTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MHacks StockXUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
